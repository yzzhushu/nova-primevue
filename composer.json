{
    "name": "jshxl/primevue",
    "description": "A Laravel Nova asset.",
    "keywords": [
        "laravel",
        "nova",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.10",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Jshxl\\Primevue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jshxl\\Primevue\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
